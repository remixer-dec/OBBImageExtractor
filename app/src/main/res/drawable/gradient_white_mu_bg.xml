<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="@color/mu_background_secondary"
                android:centerColor="@color/mu_background"
                android:endColor="@color/mu_background"
                android:angle="90"/>
        </shape>
    </item>
    <item android:bottom="180dp" android:top="80dp" android:left="-180dp" android:right="-180dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="@color/mu_background_secondary"
                android:centerColor="@color/mu_background_secondary"
                android:endColor="@color/mu_background"
                android:angle="90"/>
        </shape>
    </item>
</layer-list>
